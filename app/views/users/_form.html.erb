<head>
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="styles/sign_up.css">
  <%= stylesheet_link_tag "https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" %>
  <link rel="icon" href="img/registration.png">
</head>

<div class="text-field">
  <%= form.text_field :email, type: 'email',placeholder: 'Email',  class: 'form-control' %>
</div>

<div class="password-field">
  <%= form.password_field :password, placeholder: 'Пароль', class: 'form-control' %>
</div>
